// mesh type
// meshType can be only Mesh2D or Mesh3D
meshType=Mesh3D;
// fileType can be ANYTHING3D, TETRAHEDRON or ANYTHING2D
fileType=ANYTHING3D;


// forceTermFunction
// It can be forceTermSquare or forceTermSphere or forceTermConvergence
forceTermFunction=147*_pi*_pi*sin(7*_pi*x)*sin(7*_pi*y)*sin(7*_pi*z);
//forceTermConvergence;

// solution function
// It can be solutionSquare or solutionSphere or solutionConvergence
solutionFunction=sin(7*_pi*x)*sin(7*_pi*y)*sin(7*_pi*z);
//solutionConvergence;


