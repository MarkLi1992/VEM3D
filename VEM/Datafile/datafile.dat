// mesh type
// meshType can be only Mesh2D or Mesh3D
meshType=Mesh3D;
// fileType can be ANYTHING3D, TETRAHEDRON or ANYTHING2D
fileType=TETRAHEDRON;


// forceTermFunction
// It can be forceTermSquare or forceTermSphere
forceTermFunction=forceTermSquare;

// solution function
// It can be solutionSquare or solutionSphere
solutionFunction=solutionSquare;


// file output name
outputPoint=Output/point3D.txt;
outputConnection=Output/conn3D.txt;
outputError=Output/error3D.txt;
outputSolution=Output/solution3D.txt;

// override or append error to error file
// errorAction can only be override or append
errorAction=override

// typename real (double or long double)
real=long double;
