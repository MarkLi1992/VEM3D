// 2D MESH SQUARE
inputPoint=../Mesh/Mesh2DSquare/point-16.txt
inputConnection=../Mesh/Mesh2DSquare/conn-16.txt;

// TEST
//inputPoint=../Mesh/Test/point3.txt;
//inputConnection=../Mesh/Test/conn3.txt;


// mesh type
// meshType can be only Mesh2D or Mesh3D
meshType=Mesh2D;
// fileType can be ANYTHING3D, TETRAHEDRON or ANYTHING2D
fileType=ANYTHING2D


// forceTermFunction
// It can be forceTermSquare or forceTermSphere or forceTermConvergence
forceTermFunction=forceTermConvergence;

// solution function
// It can be solutionSquare or solutionSphere or solutionConvergence
solutionFunction=solutionConvergence;


// file output name
outputPoint=Output/point2D.txt;
outputConnection=Output/conn2D.txt;
outputSolution=Output/solution2D.txt;
outputError=Output/error2D.txt;

// override or append error to error file
// errorAction can only be override or append
errorAction=override

// typename real (double or long double)
real=long double;
